{
	"name": "pierced-react___remix-fragment",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "NODE_ENV=production remix vite:build",
		"dev": "remix vite:dev",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"serve": "wrangler dev --local --var DEV_MODE=true",
		"start": "pnpm serve --port 3000",
		"buildAndServe": "pnpm build && pnpm serve --port 3000",
		"deploy": "wrangler deploy",
		"types:check": "tsc --noEmit"
	},
	"dependencies": {
		"@remix-run/cloudflare": "^2.17.2",
		"@remix-run/react": "^2.17.2",
		"isbot": "^4.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^2.17.2",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"@cloudflare/workers-types": "^4.20240502.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.5.4",
		"vite": "^5.1.0",
		"vite-tsconfig-paths": "^4.2.1",
		"wrangler": "^4.50.0"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
